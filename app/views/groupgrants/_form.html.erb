<%= simple_form_for(@groupgrant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :goal_date %>
    <%= f.input :goal_amount %>
    <%= f.input :owner_id %>
    <%= f.input :partner_id %>
    <%= f.input :completed_date %>
    <%= f.input :is_complete %>
    <%= f.input :is_enabled %>
    <%= f.input :video_url %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
